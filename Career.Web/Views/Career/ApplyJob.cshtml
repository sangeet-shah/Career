@model ApplicationFormModel

@using Career.Data.Services.Common
@using Career.Web.Models.Career
@using Career.Data.Domains.Common
@inject ICommonService commonService

@{
    ViewBag.PageTitle = "Apply Job";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["HeadCustom"] = "<link rel='canonical' href='"+@Model.baseURL+"/careers/careerdetail/?id="+@Model.JobId+"' />";    
}

<link href="~/lib/jquery-ui/jquery-ui.min.css" rel="stylesheet">
<script src="~/lib/jquery-ui/jquery-ui.min.js"></script>

<section class="contact application-page" id="contact">
    <div class="container">
        <div class="row">
            <div class="col-md-10 col-centered">
                <h2 class="short">@Model.JobTitle</h2>
                <hr>
                <div class="store-info">
                    <p>Location: @Model.Location</p>
                    <p>Store: @Model.StoreName</p>
                    <p>Department: @Model.Department</p>
                </div>

                <div class="clearfix"></div>
                <p>@Model.GreetingText</p>
                <div class="clearfix"></div>
                <p>The Hiring Manager will contact you directly should you be selected for an interview for this position.</p>
                <div class="clearfix"></div>
                <div class="panel-group" id="panels">
                    @using (Html.BeginForm("ApplyJob", "Career", FormMethod.Post, new { enctype = "multipart/form-data", id = "apply-job",name="applyjob" }))
                    {
                        <input type="hidden" name="jobid" id="JobId" value="@Model.JobId">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <a class="accordion-toggle collapsed" data-toggle="collapse" id="pnlone" data-parent="#panels" href="#collapseOne">
                                    <h4 class="panel-title">
                                        Personal Information
                                    </h4>
                                </a>
                            </div>
                            <div id="collapseOne" class="panel-collapse collapse in">
                                <div class="panel-body">
                                    <div class="form-horizontal">
                                        <div>
                                            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-left">
                                                <div class="form-group">
                                                    <label for="name" class="col-xs-4 col-sm-3 col-md-3 col-lg-3  control-label">First Name*:</label>
                                                    <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                        @Html.TextBoxFor(c => c.FirstName, new { @class = "form-control", placeholder = "First name" })
                                                        <div class="error" id="firstNameValidMsg">@Html.ValidationMessageFor(c => c.FirstName)</div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-right">
                                                <div class="form-group">
                                                    <label for="lname" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">Last Name*:</label>
                                                    <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                        @Html.TextBoxFor(c => c.LastName, new { @class = "form-control", placeholder = "Last name" })
                                                        <div class="error" id="lastNameValidMsg">@Html.ValidationMessageFor(c => c.LastName)</div>

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="clearfix"></div>
                                        <div>
                                            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-left">
                                                <div class="form-group">
                                                    <label for="email" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">Address*:</label>
                                                    <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                        @Html.TextBoxFor(c => c.Address, new { @class = "form-control", placeholder = "Address" })
                                                        <div class="error" id="addressValidMsg">@Html.ValidationMessageFor(c => c.Address)</div>

                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-right">
                                                <div class="form-group">
                                                    <label for="city" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">City*:</label>
                                                    <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                        @Html.TextBoxFor(c => c.City, new { @class = "form-control", placeholder = "City" })
                                                        <div class="error" id="cityValidMsg">@Html.ValidationMessageFor(c => c.City)</div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="clearfix"></div>
                                        <div>
                                            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-left">
                                                <div class="form-group">
                                                    <label for="state" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">State:</label>
                                                    <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                        @Html.DropDownListFor(c => c.State, Model.States, new { @class = "form-control" })
                                                        <div class="error" id="stateValidMsg">@Html.ValidationMessageFor(c => c.State)</div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-right">
                                                <div class="form-group">
                                                    <label for="zipcode" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">Zip code*:</label>
                                                    <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                        @Html.TextBoxFor(c => c.ZipCode, new { @class = "form-control numeric", placeholder = "Zip code" })
                                                        <div class="error" id="zipCodeValidMsg">@Html.ValidationMessageFor(c => c.ZipCode)</div>

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="clearfix"></div>
                                        <div>
                                            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-left">
                                                <div class="form-group">
                                                    <label for="tel" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">Phone*:</label>
                                                    <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                        @Html.TextBoxFor(c => c.Phone, new { @class = "form-control numeric", placeholder = "Numbers only" })
                                                        @Html.ValidationMessageFor(c => c.Phone)
                                                        <div class="error" id="phoneValidMsg"></div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-right">
                                                <div class="form-group">
                                                    <label for="Email" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">Email*:</label>
                                                    <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                        @Html.TextBoxFor(c => c.Email, new { @class = "form-control", type = "email", placeholder = "Email" })
                                                        @Html.ValidationMessageFor(c => c.Email)
                                                        <div class="error" id="EmailValidMsg"></div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="clearfix"></div>
                                        <div>
                                            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-left">
                                            </div>
                                            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-right">
                                                <div class="form-group pull-right">
                                                    <div class="col-md-9 col-md-offset-3">
                                                        <button class="btn btn-primary" id="btnNextOne" name="btnnextone" type="button" href="#collapseTwo" role="button">NEXT STEP</button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <a class="accordion-toggle collapsed" data-toggle="collapse" id="pnlTwo" data-parent="#panels" href="#collapseTwo">
                                    <h4 class="panel-title">
                                        Resume & Professional Links (Optional)
                                    </h4>
                                </a>
                            </div>
                            <div id="collapseTwo" class="panel-collapse collapse">
                                <div class="panel-body">
                                    <div class="form-horizontal">
                                        <div>
                                            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-left">
                                                <div class="form-group">
                                                    <label for="name" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">Upload Resume:</label>
                                                    <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                        <input type="file" name="resumefilepath" id="ResumeFilePath" class="statelist"> @*Resume upload*@
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-right">
                                                <div class="form-group">
                                                    <label for="coverletter" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">Upload Cover Letter:</label>
                                                    <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                        <input type="file" name="upfilecoverletterpath" id="upFileCoverLetterPath" class="statelist"> @*Upload Cover Letter*@
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="clearfix"></div>
                                        <div>
                                            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-left">
                                                <div class="form-group">
                                                    <label for="linkedin" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 social-margin control-label">LinkedIn <span class="social-span"><img class="social-icon" src="~/images/linkdin-icon-large.png"></span></label>
                                                    <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                        @Html.TextBoxFor(x => x.LinkedinURL, new { type = "url", @class = "form-control", placeholder = "Please enter your LinkedIn URL" })
                                                        @Html.ValidationMessageFor(x => x.LinkedinURL)
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-right">
                                                <div class="form-group">
                                                    <label for="facebook" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 social-margin control-label">Facebook <span class="social-span"><img class="social-icon" src="~/images/facebook-icon-large.png"></span></label>
                                                    <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                        @Html.TextBoxFor(x => x.FacebookURL, new { type = "url", @class = "form-control", placeholder = "Please enter your Facebook URL" })
                                                        @Html.ValidationMessageFor(x => x.FacebookURL)
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="clearfix"></div>
                                        <div>
                                            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-left">
                                                <div class="form-group">
                                                    <label for="portfolio" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">Portfolio:</label>
                                                    <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                        @Html.TextBoxFor(x => x.PortFolioURL, new { @class = "form-control", placeholder = "Portfolio" })
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-right">
                                                <div class="social-url-list"></div>
                                            </div>
                                        </div>
                                        <div class="clearfix"></div>
                                        <div class="form-group">
                                            <div class="col-sm-offset-6 col-sm-6 col-md-offset-6 col-md-6 col-lg-offset-6 col-lg-6">
                                                <a class="btn-more btn-add-social-url pull-right"><span class="social-span"><img class="social-icon" src="~/images/add-more-icon.png"></span> Add More</a>
                                            </div>
                                        </div>
                                        <div class="form-group pull-right">
                                            <div class="col-md-9 col-md-offset-3">
                                                <button class="btn btn-primary" id="btnNextTwo" name="btnnexttwo" type="button" href="#collapseTwo" role="button">NEXT STEP</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <a class="accordion-toggle collapsed" data-toggle="collapse" id="pnlthree" data-parent="#panels" href="#collapseThree">
                                    <h4 class="panel-title">
                                        Education & Work Experience History
                                    </h4>
                                </a>
                            </div>
                            <div id="collapseThree" class="panel-collapse collapse education-history">
                                <div class="panel-body">
                                    <div class="form-horizontal">
                                        <h4>EDUCATION</h4>
                                        <div class="col-md-12">
                                            <div class="row">
                                                <div>
                                                    @Html.HiddenFor(x => x.Education.Count)
                                                    <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-left">
                                                        <div class="form-group">
                                                            <label for="name" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">Highest Education:</label>
                                                            <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                                @Html.DropDownListFor(x => x.Education.HighestEducation, new[]{
                                                                    new SelectListItem(){ Text = "Some High School", Value = "Some High School"},
                                                                    new SelectListItem(){ Text = "High School Diploma/GED", Value = "High School Diploma/GED"},
                                                                    new SelectListItem(){ Text = "Technical School", Value = "Technical School"},
                                                                    new SelectListItem(){ Text = "Some College", Value = "Some College"},
                                                                    new SelectListItem(){ Text = "Associates Degree", Value = "Associates Degree"},
                                                                    new SelectListItem(){ Text = "Bachelors Degree", Value = "Bachelors Degree"},
                                                                    new SelectListItem(){ Text = "Masters Degree", Value = "Masters Degree"},
                                                                    new SelectListItem(){ Text = "Doctorate Degree", Value = "Doctorate Degree"},
                                                                    }, new { @class = "form-control", @id = "Education_HighestEducation_" + Model.Education.Count, @name = "Education_HighestEducation_" + Model.Education.Count })
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-right">
                                                        <div class="form-group">
                                                            <label for="institution" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">Institution:</label>
                                                            <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                                @Html.TextBoxFor(x => x.Education.Institution, new { @class = "form-control", @id = "Education_Institution_" + Model.Education.Count, @name = "Education_Institution_" + Model.Education.Count, placeholder = "Institution" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="clearfix"></div>
                                                <div class="degree-zip-fields">
                                                    <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-left">
                                                        <div class="form-group">
                                                            <label for="degree" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">Degree / Accomplishments:</label>
                                                            <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                                @Html.TextBoxFor(x => x.Education.DegreeAccomplishments, new { @class = "form-control", @id = "Education_DegreeAccomplishments_" + Model.Education.Count, @name = "Education_DegreeAccomplishments_" + Model.Education.Count, placeholder = "Degree / Accomplishments" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-right">
                                                        <div class="form-group">
                                                            <label for="zipcode" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">City:</label>
                                                            <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                                @Html.TextBoxFor(x => x.Education.City, new { @class = "form-control", @id = "Education_City_" + Model.Education.Count, @name = "Education_City_" + Model.Education.Count, placeholder = "City" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="clearfix"></div>
                                                <div class="degree-zip-fields">
                                                    <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 text-field-left">
                                                        <div class="form-group">
                                                            <label for="state1" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">State:</label> <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                                @Html.DropDownListFor(c => c.Education.State, Model.States, new { @class = "form-control", @id = "Education_State_" + Model.Education.Count, @name = "Education_State_" + Model.Education.Count })
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="phone-list1"></div>
                                                <div class="form-group">
                                                    <div class="col-md-12">
                                                        <a class="btn-more pull-right btn-add-phone1"><span class="social-span"><img class="social-icon" src="~/images/add-more-icon.png"></span> Add More</a>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-12">
                                                <h4>WORK EXPERIENCE (Include 3 most recent)</h4>
                                                <div class="row set-minHeight">
                                                    @Html.HiddenFor(x => x.WorkExperience.Count)
                                                    <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                                        <div class="form-group">
                                                            <label for="job-title" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">Job Title*:</label>
                                                            <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                                @Html.TextBoxFor(x => x.WorkExperience.JobTitle, new { @class = "form-control", placeholder = "Job Title" })
                                                                <div class="error" id="woJobTitleValidMsg"></div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                                        <div class="form-group">
                                                            <label for="company" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">Company*:</label>
                                                            <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                                @Html.TextBoxFor(x => x.WorkExperience.Company, new { @class = "form-control", placeholder = "Company" })
                                                                <div class="error" id="woCompanyValidMsg"></div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="clearfix"></div>
                                                    <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                                        <div class="form-group">
                                                            <label for="state1" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">State:</label> <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                                @Html.DropDownListFor(c => c.WorkExperience.State, Model.States, new { @class = "form-control" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">

                                                        <div class="form-group">
                                                            <label for="city1" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">City*:</label>
                                                            <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                                @Html.TextBoxFor(x => x.WorkExperience.City, new { @class = "form-control", placeholder = "City" })
                                                                <div class="error" id="woCityValidMsg"></div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="clearfix"></div>
                                                    <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                                        <div class="form-group">
                                                            <label for="startdate1" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">Start Date:</label>
                                                            <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                                @Html.TextBoxFor(x => x.WorkExperience.StartDate, "{0:MM-dd-yyyy}", new { @class = "form-control datepicker-icon readOnlyDate", autocomplete = "off", @readonly = "readonly" })
                                                                @Html.ValidationMessageFor(c => c.WorkExperience.StartDate)
                                                                <div class="error" id="woStartDateValidMsg"></div>
                                                                <script>
                                                                    $(document).ready(function () {
                                                                        $('#WorkExperience_StartDate').datepicker({
                                                                            changeMonth: true,
                                                                            changeYear: true,
                                                                            yearRange: "-200:+0",
                                                                            maxDate: 'now'
                                                                        });
                                                                    });
                                                                </script>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                                        <div class="form-group">
                                                            <label for="enddate1" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">End Date:</label> <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                                @Html.TextBoxFor(x => x.WorkExperience.EndDate, "{0:MM-dd-yyyy}", new { @class = "form-control datepicker-icon readOnlyDate", autocomplete = "off", @readonly = "readonly" })
                                                                @Html.ValidationMessageFor(c => c.WorkExperience.EndDate)
                                                                <div class="error" id="woEndDateValidMsg"></div>
                                                                <script>
                                                                    $(document).ready(function () {
                                                                        $('#WorkExperience_EndDate').datepicker({
                                                                            changeMonth: true,
                                                                            changeYear: true,
                                                                            yearRange: "-200:+0",
                                                                            maxDate: 'now'
                                                                        });
                                                                    });
                                                                </script>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="clearfix"></div>
                                                    @if (!string.IsNullOrEmpty(Model.Location) && Model.Location.Trim().ToLower() != "il")
                                                    {
                                                        <div id="woPayUpOnLeavingContainer" class="col-xs-12 col-sm-6 col-md-6 col-lg-6 woPayUpOnLeaving">
                                                            <div class="form-group">
                                                                <label for="pay" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">Pay Upon Leaving*:</label>
                                                                <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                                    @Html.TextBoxFor(x => x.WorkExperience.PayUpOnLeaving, new { @class = "form-control", placeholder = "Pay Upon Leaving" })
                                                                    <div class="error" id="woPayUpOnLeavingValidMsg"></div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    }
                                                    <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                                        <div class="form-group">
                                                            <label for="reason" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">Reason for Leaving*:</label>
                                                            <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                                @Html.TextBoxFor(x => x.WorkExperience.ResionForLeaving, new { @class = "form-control", placeholder = "Reason for Leaving" })
                                                                <div class="error" id="woResionForLeavingValidMsg"></div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="clearfix"></div>
                                                    <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                                        <div class="form-group">
                                                            <label for="city1" class="col-xs-4 col-sm-3 col-md-3 col-lg-3 control-label">Supervisor*:</label>
                                                            <div class="col-xs-8 col-sm-9 col-md-9 col-lg-9">
                                                                @Html.TextBoxFor(x => x.WorkExperience.Supervisor, new { @class = "form-control", placeholder = "Supervisor" })
                                                                <div class="error" id="woSupervisorValidMsg"></div>
                                                            </div>
